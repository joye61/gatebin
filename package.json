{
  "name": "gatebin",
  "version": "0.1.1",
  "description": "Binary Gateway Front End SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "start": "npm run clear && tsc && concurrently --timings npm:watch:ts npm:watch:test",
    "watch:ts": "tsc --pretty -w",
    "watch:test": "npm run test -- --watch",
    "build": "npm run clear && tsc",
    "clear": "rm -rfv ./dist",
    "test": "cross-env TS_JEST_DISABLE_VER_CHECKER=true jest --config ./jest.config.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joye61/gatebin"
  },
  "keywords": [
    "bin",
    "gateway"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "author": "joye",
  "license": "MIT",
  "devDependencies": {
    "@types/content-type": "^1.1.5",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/pako": "^1.0.3",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "content-type": "^1.0.4",
    "pako": "^2.0.4"
  }
}
